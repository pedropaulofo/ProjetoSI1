# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Faz a chamada do Index de Autenticacao do Usuario
GET     /                           controllers.AutenticacaoController.index
GET     /home                       controllers.HorariosController.index
POST    /cadastroRealizado	        controllers.AutenticacaoController.cadastraUsuario
POST    /cadastroHorarios         	controllers.AutenticacaoController.efetuaLogin
#POST   /uploadFotoUsuario          controllers.AutenticacaoController.uploadFotoUsuario
GET     /logout                     controllers.AutenticacaoController.efetuaLogout


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
